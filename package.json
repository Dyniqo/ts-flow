  },
  "description": "",
  "main": "dist/ts-flow.cjs.js",
  "module": "dist/ts-flow.esm.js",
  "types": "dist/ts-flow.d.ts",
  "exports": {
    ".": {
      "require": "./dist/ts-flow.cjs.js",
      "import": "./dist/ts-flow.esm.js",
      "types": "./dist/ts-flow.d.ts"
    },
