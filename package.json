    "cron scheduler",
    "parallel tasks",
    "retry policies",
    "state management",
    "hooks",
    "error handling",
    "es6 modules",
    "iife",
    "cjs",
    "esm"
  ],
  "homepage": "https://github.com/Dyniqo/ts-flow",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Dyniqo/ts-flow.git"
  },
  "bugs": {
    "url": "https://github.com/Dyniqo/ts-flow/issues"
  },
